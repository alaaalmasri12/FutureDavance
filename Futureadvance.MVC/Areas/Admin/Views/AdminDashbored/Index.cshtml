@{
    ViewData["Title"] = "users items";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IReadOnlyList<Localuser>;
<div class="mt-3 ">

    <div class="container-fluid">
        <div class="row pt-4 pb-3">
            <div class="col-6">
                <h2 class="text-primary">Users List</h2>
            </div>
        </div>
    </div>
    <div class="container-fluid">

       
            <div class="block margin-bottom-sm">
                <table class="table" id="userstable">
                    <thead>
                        <tr>
                        <th>userName</th>
                        <th>email</th>
                        <th>Role</th>
                         <th>Action</th>
                        </tr>
                    </thead>

                </table>
            </div>
    </div>

</div>



@section scripts {

    <script>
        $(document).ready(function () {
            loadusers();
        })
    </script>
}
